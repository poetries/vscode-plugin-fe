{
    "name": "fe-tools",
    "displayName": "前端进阶之旅",
    "description": "前端面试指南宝典、前端人常用的工具箱",
    "version": "1.0.8",
    "icon": "resources/icon.png",
    "license": "GPL",
    "publisher": "poetries",
    "repository": {
        "url": "https://github.com/poetries/vscode-plugin-fe.git"
    },
    "keywords": [
        "前端面试宝典",
        "前端面试指南",
        "前端面试之旅",
        "前端进阶之旅",
        "前端工具箱",
        "feinterview",
        "frontend",
        "工具箱",
        "前端求职"
    ],
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:feinterview_site",
        "onView:feinterview_tools",
        "onView:feinterview_docs",
        "onView:feinterview_blog",
        "onView:feinterview_weapparticle",
        "onView:feinterview_collect",
        "onView:feinterview_setting"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "feinterview.addTools",
                "title": "更多工具",
                "icon": "resources/icon_appadd.svg"
            },
            {
                "command": "feinterview.viewAllWeappArticle",
                "title": "查看全部",
                "icon": "resources/icon_view.svg"
            },
            {
                "command": "feinterview.refreshTopic",
                "title": "换一批",
                "icon": "resources/icon_refresh.svg"
            },
            {
                "command": "feinterview.refreshQuestion",
                "title": "换一批",
                "icon": "resources/icon_refresh.svg"
            },
            {
                "command": "feinterview.addCollect",
                "title": "添加网址",
                "icon": {
                    "light": "resources/light/add.svg",
                    "dark": "resources/dark/add.svg"
                }
            },
            {
                "command": "feinterview.deleteCollect",
                "title": "删除网址",
                "icon": {
                    "light": "resources/light/del.svg",
                    "dark": "resources/dark/del.svg"
                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "feinterview",
                    "title": "前端进阶之旅",
                    "icon": "resources/icon_fe.svg"
                }
            ]
        },
        "views": {
            "feinterview": [
                {
                    "id": "feinterview_site",
                    "name": "快捷导航"
                },
                {
                    "id": "feinterview_docs",
                    "name": "文档查询"
                },
                {
                    "id": "feinterview_tools",
                    "name": "常用工具"
                },
                               {
                    "id": "feinterview_weapparticle",
                    "name": "文章动态"
                },
                {
                    "id": "feinterview_blog",
                    "name": "前端资讯"
                },
                {
                    "id": "feinterview_collect",
                    "name": "收藏夹"
                },
                {
                    "id": "feinterview_setting",
                    "name": "关于我们"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "feinterview.addTools",
                    "when": "view == feinterview_tools",
                    "group": "navigation@1"
                },
                {
                    "command": "feinterview.viewAllWeappArticle",
                    "when": "view == feinterview_weapparticle",
                    "group": "navigation@1"
                },
                {
                    "command": "feinterview.addCollect",
                    "when": "view == feinterview_collect",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "feinterview.deleteCollect",
                    "when": "view == feinterview_collect",
                    "group": "group1"
                },
                {
                    "command": "feinterview.deleteCollect",
                    "when": "view == feinterview_collect",
                    "group": "inline"
                }
            ]
        },
        "configuration": {
            "properties": {
                "frontend-box.markbook": {
                    "type": "array",
                    "default": [
                        {
                            "title": "前端进阶之旅",
                            "url": "https://feinterview.poetries.top"
                        },
                        {
                            "title": "阮一峰科技爱好者周刊",
                            "url": "https://github.com/ruanyf/weekly/issues"
                        }
                    ],
                    "description": "收藏网址"
                }
            }
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "yarn run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.35.0",
        "eslint": "^7.6.0",
        "glob": "^7.1.6",
        "mocha": "^8.0.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "events": "^3.2.0"
    }
}
